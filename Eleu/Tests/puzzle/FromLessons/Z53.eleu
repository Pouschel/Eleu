var p = "
:def
c=CatW
p=101
m=10101
s='00110010
g=Green Square
:grid
pcg
:meta
win = Val 2 0 5
name = Z53 - Binär nach Dezimal
funcs = move read write turn
:info
Lies die Binärzahl ein und wandle sie in eine Dezimalzahl um.

Schreibe das Ergebnis in das grüne Feld
:grid
mcg
:meta
win = Val 2 0 21
:grid
scg
:meta
win = Val 2 0 50
hash = c7c734a829
";

_puzzle(p, 2);

var s = read();
var länge = len(s);
var erg = 0;
var n=0;
repeat(länge)
{
  erg = 2 * erg;
  erg = erg + parseInt(substr(s, n, 1));
  n=n+1;
}
turn("around");
write(erg);

assert _isSolved();



