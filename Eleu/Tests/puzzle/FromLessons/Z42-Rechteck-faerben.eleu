fun s2(ti)
{
  assert _puzzle(p, ti);
  var rows = parseInt(read()); move();
  var cols = parseInt(read()); move();
  repeat(rows)
  {
    move("S");
    repeat(cols)
    {
      paint("blue");
      move();
    }
    turn("around");move();
  }
  assert _isSolved();
}
