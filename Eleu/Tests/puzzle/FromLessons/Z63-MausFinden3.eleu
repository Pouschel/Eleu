var p="
:def
c=Cat
m=Mouse
b=Bowl
:grid
.
.....b..m
..m........b
.m......b
.....m....b
...b....m
....b......m.
cm.......b
:meta
win=MiceInBowls
name=Z63 - Mäuse im Feld finden
score=2000
funcs=move take drop seeing turn
:info
Finde die Mäuse und bringe sie in die Näpfe.
";

fun s1()
{
  assert _puzzle(p);
  repeat(7)
  {
    repeat(11)
    {
      if (seeing()=="Mouse")
        take();
      move();  
    }
    repeat(11)
    {
      if (seeing()=="Bowl")
        drop();
      move("W");
    }
    move("N");
  }
  assert _isSolved(); 
}
s1();
  