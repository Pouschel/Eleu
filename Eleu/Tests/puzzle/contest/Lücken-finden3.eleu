fun solve(idx)
{
	_puzzle(p, idx);
	move();
	while (true)
	{
		turn("left");
		if (seeing()!= "Wall")
		{
			move();
			if (color()== "Green")
				break;
			move("S");
		}
		turn("around");
		if (seeing()!= "Wall")
		{
			move();
			if (color()== "Green")
				break;
      move("N");
		}
		turn("left");
		move(); move();
	}
	assert _isSolved();
}

